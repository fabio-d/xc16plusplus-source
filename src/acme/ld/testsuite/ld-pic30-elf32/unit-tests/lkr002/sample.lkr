CRT0_STARTUP(crt0_standard.o)
CRT1_STARTUP(crt1_standard.o)
CRT_STARTMODE(crt_start_mode_normal)

MEMORY
{
  program (rx) : ORIGIN = 0,    LENGTH = 0x8000
  data  (a!rx)  : ORIGIN = 0,    LENGTH = 0x8000
}

INCLUDE provides.lkr

SECTIONS
{
.text ALIGN(4) :
  {
	*(.text);
  } >program

.bss ALIGN(2) :
  {
        *(.bss);
  } >data

.data ALIGN(2) :
  {
        *(.data);
  } >data

.ybss MAX( 0x3000 , ALIGN(2)) :
  {
        *(.ybss);
  } >data

}
